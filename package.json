  "description": "Tutor Connect - Next.js Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "next build",
    "build:static": "next build",
    "build:production": "next build",
    "build:production:static": "next build",
    "start": "next start",
