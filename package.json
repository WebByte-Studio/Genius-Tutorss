    "start:backend": "cd backend && npm start",
    "start:production:backend": "cd backend && npm run start:production",
    "deploy:static": "npm run build:production:static",
    "deploy:static:full": "npm run deploy:static && npm run start:static",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out",
    "clean:all": "rimraf .next out node_modules && npm install"
